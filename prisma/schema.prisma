datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
  output          = "../node_modules/.prisma/client"
  previewFeatures = ["jsonProtocol"]
}

model Booking {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  phone     String
  date      DateTime
  time      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}